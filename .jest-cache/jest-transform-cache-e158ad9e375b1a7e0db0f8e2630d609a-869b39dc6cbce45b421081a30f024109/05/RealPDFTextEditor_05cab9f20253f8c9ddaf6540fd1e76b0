      try {
        const page =
        /* istanbul ignore next */
        (cov_g5kivdxmb().s[323]++, await pdf.getPage(pageNum));
        const textContent =
        /* istanbul ignore next */
        (cov_g5kivdxmb().s[324]++, await page.getTextContent());
