      cov_2ogk7e01dh().s[51]++;
      handleSave();
    } else {
      /* istanbul ignore next */
      cov_2ogk7e01dh().b[18][1]++;
      cov_2ogk7e01dh().s[52]++;
      if (e.key === 'Escape') {
        /* istanbul ignore next */
        cov_2ogk7e01dh().b[20][0]++;
        cov_2ogk7e01dh().s[53]++;
        e.preventDefault();
        /* istanbul ignore next */
        cov_2ogk7e01dh().s[54]++;
        handleCancel();
      } else
      /* istanbul ignore next */
      {
        cov_2ogk7e01dh().b[20][1]++;
      }
    }
  };
  /* istanbul ignore next */
  cov_2ogk7e01dh().s[55]++;
  const handleSave = () => {
    /* istanbul ignore next */
    cov_2ogk7e01dh().f[12]++;
    cov_2ogk7e01dh().s[56]++;
    if (text.trim() !== region.originalText) {
      /* istanbul ignore next */
      cov_2ogk7e01dh().b[21][0]++;
      cov_2ogk7e01dh().s[57]++;
      onSave(text.trim());
    } else {
      /* istanbul ignore next */
      cov_2ogk7e01dh().b[21][1]++;
      cov_2ogk7e01dh().s[58]++;
      onCancel();
    }
  };
  /* istanbul ignore next */
  cov_2ogk7e01dh().s[59]++;
  const handleCancel = () => {
    /* istanbul ignore next */
    cov_2ogk7e01dh().f[13]++;
    cov_2ogk7e01dh().s[60]++;
    setText(region.originalText);
    /* istanbul ignore next */
    cov_2ogk7e01dh().s[61]++;
    onCancel();
  };
  /* istanbul ignore next */
  cov_2ogk7e01dh().s[62]++;
  const handleBlur = () => {
    /* istanbul ignore next */
    cov_2ogk7e01dh().f[14]++;
    cov_2ogk7e01dh().s[63]++;
    setIsFocused(false);
    // Auto-save on blur if text changed
    /* istanbul ignore next */
    cov_2ogk7e01dh().s[64]++;
    if (text.trim() !== region.originalText) {
      /* istanbul ignore next */
      cov_2ogk7e01dh().b[22][0]++;
      cov_2ogk7e01dh().s[65]++;
      handleSave();
    } else {
      /* istanbul ignore next */
      cov_2ogk7e01dh().b[22][1]++;
      cov_2ogk7e01dh().s[66]++;
      handleCancel();
    }
  };
  // Calculate position to stay within viewport
  /* istanbul ignore next */
  cov_2ogk7e01dh().s[67]++;
  const calculatePosition = () => {
    /* istanbul ignore next */
    cov_2ogk7e01dh().f[15]++;
    const viewport =
    /* istanbul ignore next */
    (cov_2ogk7e01dh().s[68]++, {
      width: window.innerWidth,
      height: window.innerHeight
    });
    let left =
    /* istanbul ignore next */
    (cov_2ogk7e01dh().s[69]++, position.x);
    let top =
    /* istanbul ignore next */
    (cov_2ogk7e01dh().s[70]++, position.y);
    // Adjust horizontal position
    /* istanbul ignore next */
    cov_2ogk7e01dh().s[71]++;
    if (left + 250 > viewport.width) {
      /* istanbul ignore next */
      cov_2ogk7e01dh().b[23][0]++;
      cov_2ogk7e01dh().s[72]++;
      left = viewport.width - 260;
    } else
    /* istanbul ignore next */
    {
      cov_2ogk7e01dh().b[23][1]++;
    }
    cov_2ogk7e01dh().s[73]++;
    if (left < 10) {
      /* istanbul ignore next */
      cov_2ogk7e01dh().b[24][0]++;
      cov_2ogk7e01dh().s[74]++;
      left = 10;
    } else
    /* istanbul ignore next */
    {
      cov_2ogk7e01dh().b[24][1]++;
    }
    // Adjust vertical position  
    cov_2ogk7e01dh().s[75]++;
    if (top + 100 > viewport.height) {
      /* istanbul ignore next */
      cov_2ogk7e01dh().b[25][0]++;
      cov_2ogk7e01dh().s[76]++;
      top = position.y - 100;
    } else
    /* istanbul ignore next */
    {
      cov_2ogk7e01dh().b[25][1]++;
    }
    cov_2ogk7e01dh().s[77]++;
    if (top < 10) {
      /* istanbul ignore next */
      cov_2ogk7e01dh().b[26][0]++;
      cov_2ogk7e01dh().s[78]++;
      top = 10;
    } else
    /* istanbul ignore next */
    {
      cov_2ogk7e01dh().b[26][1]++;
    }
    cov_2ogk7e01dh().s[79]++;
    return {
      left,
      top
    };
  };
  const editorPosition =
  /* istanbul ignore next */
  (cov_2ogk7e01dh().s[80]++, calculatePosition());
  const scaledWidth =
  /* istanbul ignore next */
  (cov_2ogk7e01dh().s[81]++, Math.max(region.width * zoom, 100)); // Minimum width
  const scaledHeight =
  /* istanbul ignore next */
  (cov_2ogk7e01dh().s[82]++, Math.max(region.height * zoom, 20)); // Minimum height
  const scaledFontSize =
  /* istanbul ignore next */
  (cov_2ogk7e01dh().s[83]++, Math.max(region.fontSize * zoom, 10)); // Minimum font size
  /* istanbul ignore next */
  cov_2ogk7e01dh().s[84]++;
  return (0, jsx_runtime_1.jsxs)("div", {
    className: "inline-text-editor",
    style: {
      position: 'fixed',
      left: editorPosition.left,
      top: editorPosition.top,
      width: Math.max(scaledWidth, 250),
      minHeight: scaledHeight,
      zIndex: 1000,
      background: 'white',
      border: '2px solid #007bff',
      borderRadius: '6px',
      boxShadow: '0 4px 12px rgba(0,0,0,0.3)',
      padding: '8px'
    },
    children: [(0, jsx_runtime_1.jsx)("textarea", {
      ref: textareaRef,
      value: text,
      onChange: e => {
        /* istanbul ignore next */
        cov_2ogk7e01dh().f[16]++;
        cov_2ogk7e01dh().s[85]++;
        return setText(e.target.value);
      },
      onKeyDown: handleKeyDown,
      onFocus: () => {
        /* istanbul ignore next */
        cov_2ogk7e01dh().f[17]++;
        cov_2ogk7e01dh().s[86]++;
        return setIsFocused(true);
      },
      onBlur: handleBlur,
      className: "text-editor-input",
      style: {
        width: '100%',
        minHeight: scaledHeight,
        fontSize: scaledFontSize,
        fontFamily:
        /* istanbul ignore next */
        (cov_2ogk7e01dh().b[27][0]++, region.fontName) ||
        /* istanbul ignore next */
        (cov_2ogk7e01dh().b[27][1]++, 'Arial, sans-serif'),
        resize: 'both',
        border: '2px solid #007bff',
        borderRadius: '3px',
        padding: '2px 4px',
        background: '#fff',
        boxShadow: '0 2px 8px rgba(0,0,0,0.2)',
        outline: 'none'
      },
      rows: Math.max(1, Math.ceil(text.length / 50)),
      autoFocus: true
    }), (0, jsx_runtime_1.jsxs)("div", {
      className: "edit-controls",
      children: [(0, jsx_runtime_1.jsx)("button", {
        className: "save-btn",
        onClick: handleSave,
        title: "Save changes (Enter)",
        children: "\u2713"
      }), (0, jsx_runtime_1.jsx)("button", {
        className: "cancel-btn",
        onClick: handleCancel,
        title: "Cancel changes (Escape)",
        children: "\u2715"
      })]
    }), (0, jsx_runtime_1.jsxs)("div", {
      className: "editor-info",
      children: [(0, jsx_runtime_1.jsxs)("span", {
        className: "char-count",
        children: [text.length, " chars"]
      }), (0, jsx_runtime_1.jsxs)("span", {
        className: "position-info",
        children: ["Page ", region.pageIndex + 1]
      })]
    })]
  });
};
/* istanbul ignore next */
cov_2ogk7e01dh().s[87]++;
exports.default = InlineTextEditor;
