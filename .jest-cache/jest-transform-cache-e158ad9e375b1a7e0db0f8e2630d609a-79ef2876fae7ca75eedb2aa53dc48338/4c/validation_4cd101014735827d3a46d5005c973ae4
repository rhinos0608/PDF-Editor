/**
 * Validate file path
 * @param {string} path - File path to validate
 * @returns {boolean} - True if valid, false otherwise
 */
export function isValidPath(path) {
  /* istanbul ignore next */
  cov_2q8xccjpsi().f[0]++;
  cov_2q8xccjpsi().s[1]++;
  if (
  /* istanbul ignore next */
  (cov_2q8xccjpsi().b[1][0]++, !path) ||
  /* istanbul ignore next */
  (cov_2q8xccjpsi().b[1][1]++, typeof path !== 'string')) {
    /* istanbul ignore next */
    cov_2q8xccjpsi().b[0][0]++;
    cov_2q8xccjpsi().s[2]++;
    return false;
  } else
  /* istanbul ignore next */
  {
    cov_2q8xccjpsi().b[0][1]++;
  }
  // Prevent path traversal attacks
  cov_2q8xccjpsi().s[3]++;
  if (
  /* istanbul ignore next */
  (cov_2q8xccjpsi().b[3][0]++, path.includes('..')) ||
  /* istanbul ignore next */
  (cov_2q8xccjpsi().b[3][1]++, path.includes('~'))) {
    /* istanbul ignore next */
    cov_2q8xccjpsi().b[2][0]++;
    cov_2q8xccjpsi().s[4]++;
    return false;
  } else
  /* istanbul ignore next */
  {
    cov_2q8xccjpsi().b[2][1]++;
  }
