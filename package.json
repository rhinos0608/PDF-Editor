{
  "name": "professional-pdf-editor",
  "version": "1.0.0",
  "description": "Professional PDF Editor with comprehensive features",
  "main": "dist/main/main.js",
  "scripts": {
    "start": "ts-node scripts/build.ts && cross-env NODE_ENV=production electron .",
    "dev": "ts-node scripts/dev.ts",
    "build": "ts-node scripts/build.ts",
    "build:dev": "ts-node scripts/build.ts --dev --clean",
    "build:prod": "ts-node scripts/build.ts --clean",
    "test": "ts-node scripts/test.ts",
    "test:watch": "ts-node scripts/test.ts --watch",
    "test:coverage": "ts-node scripts/test.ts --coverage",
    "validate": "ts-node scripts/validate.ts",
    "verify-build": "ts-node scripts/verify-build.ts",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "clean": "rimraf dist release",
    "clean:all": "rimraf dist release node_modules package-lock.json",
    "type-check": "tsc --noEmit",
    "postinstall": "electron-builder install-app-deps",
    "dist": "electron-builder",
    "dist:win": "electron-builder --win",
    "dist:mac": "electron-builder --mac",
    "dist:linux": "electron-builder --linux",
    "dist:all": "electron-builder --win --mac --linux"
  },
  "keywords": [
    "pdf",
    "editor",
    "electron",
    "react",
    "typescript"
  ],
  "author": "Professional PDF Editor Team",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.10",
    "@types/react-color": "^3.0.13",
    "@types/react-dom": "^19.1.7",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "babel-loader": "^10.0.0",
    "concurrently": "^9.1.0",
    "copy-webpack-plugin": "^13.0.1",
    "cross-env": "^10.0.0",
    "css-loader": "^7.1.0",
    "electron": "^37.2.6",
    "electron-builder": "^26.0.12",
    "eslint": "^9.16.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "html-webpack-plugin": "^5.6.4",
    "jest": "^30.0.5",
    "process": "^0.11.10",
    "rimraf": "^6.0.1",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "webpack": "^5.101.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  },
  "dependencies": {
    "@babel/preset-react": "^7.26.3",
    "@dwmkerr/terminal-ai": "^0.14.0",
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@fortawesome/fontawesome-free": "^7.0.0",
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@mui/material": "^7.3.1",
    "@pdf-lib/fontkit": "^1.1.1",
    "@types/bcrypt": "^6.0.0",
    "argon2": "^0.44.0",
    "bcrypt": "^6.0.0",
    "buffer": "^6.0.3",
    "dompurify": "^3.2.6",
    "electron-log": "^5.2.2",
    "electron-store": "^10.0.0",
    "electron-updater": "^6.3.9",
    "file-saver": "^2.0.5",
    "fs-extra": "^11.2.0",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.539.0",
    "path-browserify": "^1.0.1",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^5.4.54",
    "react": "^19.1.1",
    "react-color": "^2.19.3",
    "react-dom": "^19.1.1",
    "react-hotkeys-hook": "^5.1.0",
    "react-resizable-panels": "^3.0.4",
    "react-toastify": "^11.0.5",
    "tesseract.js": "^6.0.1",
    "util": "^0.12.5",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zustand": "^5.0.7"
  }
}
